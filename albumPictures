    class Program
    {
        static void Main(string[] args)
        {

            int albumSize = 52;
            int rowSize = 3;
            int fullRow = albumSize / rowSize;
            int restPictures = albumSize % rowSize;
            Console.WriteLine(fullRow);
            Console.WriteLine(restPictures);
        }
    }
