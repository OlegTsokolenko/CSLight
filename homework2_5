using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace homework2_5
{
    class Program
    {
        static void Main(string[] args)
        {
 
            int firstElement = 7;
            int lastElement = 98;
            int step = 7;

            for (int i = firstElement; i <= lastElement; i+= step) 
            {
                Console.WriteLine(i);
            }
        }
    }
}
